# Password from: aws secretsmanager get-secret-value --secret-id 'arn:aws:secretsmanager:ap-northeast-2:...:secret:rds!cluster-...'
DATABASE_URL=postgresql://postgres:<URL_ENCODED_PASSWORD>@mirae-rdb.cluster-xxx.ap-northeast-2.rds.amazonaws.com:5432/mirae_etf?sslmode=verify-full&sslrootcert=certs/global-bundle.pem
DATABASE_URL_READER=postgresql://postgres:<URL_ENCODED_PASSWORD>@mirae-rdb.cluster-ro-xxx.ap-northeast-2.rds.amazonaws.com:5432/mirae_etf?sslmode=verify-full&sslrootcert=certs/global-bundle.pem
LOG_LEVEL=INFO
DATA_DIR=./data

# GraphRAG - Graph Store (Neptune Database)
GRAPH_STORE=neptune-db://db-neptune-graphrag.cluster-xxx.ap-northeast-2.neptune.amazonaws.com
GRAPH_STORE_READER=neptune-db://db-neptune-graphrag.cluster-ro-xxx.ap-northeast-2.neptune.amazonaws.com

# GraphRAG - Vector Store (OpenSearch Serverless)
VECTOR_STORE=aoss://YOUR_COLLECTION_ID.ap-northeast-2.aoss.amazonaws.com

# AWS Region
AWS_DEFAULT_REGION=ap-northeast-2
GRAPHRAG_AWS_REGION=ap-northeast-2

# GraphRAG - LLM (AWS Bedrock model IDs)
GRAPHRAG_EXTRACTION_LLM=us.anthropic.claude-3-7-sonnet-20250219-v1:0
GRAPHRAG_RESPONSE_LLM=us.anthropic.claude-3-7-sonnet-20250219-v1:0
GRAPHRAG_EMBEDDING_MODEL=amazon.titan-embed-text-v2:0
